// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}

model Institution{
  id Int @id @default(autoincrement())
  email String
  password String
  name String
  description String?
  street String
  houseNumber Int
  complement String?
  zipCode String
  image String?
  lat Int?
  lng Int?
  createdAt DateTime @default(now())
}